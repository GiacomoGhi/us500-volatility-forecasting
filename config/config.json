{
    "io": {
        "training_file": "./data/train.csv",
        "validation_file": "./data/val.csv",
        "test_file": "./data/test.csv",
        "out_folder": "./out"
    },
    "parameters": {
        "train": true,
        "test": true,
        "show_preview": false
    },
    "hyper_parameters": {
        "batch_size": 1,
        "window_size": 40,
        "epochs": 1,
        "learning_rate": 0.01,
        "momentum": 0
    },
    "lstm_parameters": {
        "hidden_size": 1,
        "num_layers": 1
    },
    "train_parameters": {
        "network_type": "net_1",
        "step_monitor": 100,
        "loss_target": 0.001,
        "reload_last_model": false
    },
    "early_stop_parameters": {
        "loss_evaluation_epochs": 1,
        "start_epoch": 5,
        "patience": 3,
        "improvement_rate": 5
    }
}